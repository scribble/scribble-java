import Order;
import CreditCheck;
import CreditOk;
import Confirmation;
import OutOfStock;
import InsufficientCredit;

protocol MultiPartyInteractionsAndChoice@CreditAgency(role Broker) {

	CreditCheck from Broker;
	
	choice at CreditAgency {
		CreditOk to Broker;
	} or {
		InsufficientCredit to Broker;
	}
}
