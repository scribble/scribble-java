module Test;

type <java> "java.lang.Integer" from "rt.jar" as Int;

global protocol Proto(role A, role B)
{
	choice at A
	{
		1(Int) from A to B;
		1a(Int) from A to B;
		1("A!{l1():mu X . A?{l3(int): #X}, l2():end}") from A to B;
		rec X 
		{
			choice at B
			{
				3(Int) from B to A;
				do Proto(A, B);
			}
			or
			{
				4(Int) from B to A;
				4b(Int) from A to B;
			}
		}
	}
	or
	{
		2() from A to B;
	}
	//5() from A to B;
	//1("A!{l1():A?{l3(int):end},l2():end}") from A to B;
	//1(Int) from A to B;
}

